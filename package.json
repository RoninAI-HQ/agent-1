{
  "name": "blog-agent",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=@blog-agent/server\" \"npm run dev --workspace=@blog-agent/client\"",
    "dev:server": "npm run dev --workspace=@blog-agent/server",
    "dev:client": "npm run dev --workspace=@blog-agent/client",
    "build": "npm run build --workspace=@blog-agent/client"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}
